# Change nodes to adjust to your environments

[nodes]
k8s-master ansible_host=${K8S_MASTER_IP}
k8s-node1 ansible_host=${K8S_NODE1_IP}
k8s-node2 ansible_host=${K8S_NODE2_IP}
k8s-node3 ansible_host=${K8S_NODE3_IP}
[workers]
k8s-node1 ansible_host=${K8S_NODE1_IP}
k8s-node2 ansible_host=${K8S_NODE2_IP}
k8s-node3 ansible_host=${K8S_NODE3_IP}
[masters]
k8s-master

[single_master]
k8s-master

[all:vars]
ansible_ssh_user=ubuntu